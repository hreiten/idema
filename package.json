      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@bekk/storybook": "^2.5.1",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
